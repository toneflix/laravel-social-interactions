{
    "name": "toneflix-code/social-interactions",
    "description": "A Laravel package to add social interactions to your model",
    "keywords": [
        "social",
        "like",
        "saves",
        "dislike",
        "bookmark",
        "reactions",
        "interact",
        "interactions",
        "toneflix-code",
        "social-interactions"
    ],
    "homepage": "https://github.com/toneflix/laravel-social-interactions",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Toneflix Code",
            "email": "code@toneflix.com.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "2.x-dev",
        "laravel/pint": "^1.15",
        "fakerphp/faker": "^1.23",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^8.8",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ToneflixCode\\SocialInteractions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToneflixCode\\SocialInteractions\\Tests\\": "tests",
            "ToneflixCode\\SocialInteractions\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-pay-pocket --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToneflixCode\\SocialInteractions\\SocialInteractionsServiceProvider"
            ],
            "aliases": {
                "SocialInteractions": "ToneflixCode\\SocialInteractions\\Facades\\SocialInteraction"
            }
        }
    }
}